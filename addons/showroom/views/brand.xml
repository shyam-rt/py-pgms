<odoo>
  <data>
    <record model="ir.ui.view" id="showroom_brand">
      <field name="name">showroom brand</field>
      <field name="model">showroom.brand</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
        </tree>
      </field>
    </record>
  

    
    <record model="ir.actions.act_window" id="action_brand">
      <field name="name">brand</field>
      <field name="res_model">showroom.brand</field>
      <field name="view_mode">tree,form</field>
    </record>

    <menuitem name="Brand" id="showroom_brand" parent="showroom.menu_root" action="action_brand" sequence="1"/>
  </data>
</odoo>